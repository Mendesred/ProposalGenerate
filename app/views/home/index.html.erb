														<td class="showFonteSize"><%=@select_all_calculation.pct_inss_sob_soma_decimo_um_terco_ferias%><span>%.</span></td>
														<td class="showFonteSize"><%=number_to_currency(@proposal.total_inss_sob_ferias_um_terco_decimo)%></td>
