<!--<%# @select_all_calculation.salario_minimo %>-->
<div class="showFonteSize">

  <div class="panel panel-default">
    <div class="panel-heading" style="padding-bottom: 0;padding-top: 0;">
      <div class="col-lg-12">
        <div class="row">
          <div class="inline"> 
            <div class="col-xs-6">
              <!--strong><#%= t("activerecord.models.proposals.dias_jornada_ex_semana_all") %></strong-->
              <h3 style="margin-top: 10px;">Cliente:</b><%= @proposal.cliente %></h3>
            </div>
            <div class="col-xs-6"> 
              <h3 class="pull-right" style="margin-top: 10px;color: #4946e6;">Proposta: <%= @proposal.codigo_cliente %></h3>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="panel-body">
      <div class="col-lg-12">
        <div class="inline">
          <div class="row">
            <div class="col-sm-4">  
              <p class="showFonteSize">
                <strong>Data de criação:</strong>
                <%= criate_date(@proposal.created_at) %>
              </p>
            </div>
            <div class="col-sm-4">  
              <p class="showFonteSize">
                <strong>Cidade:</strong>
                <%= (@proposal.city.nome) %>
              </p>
            </div>
          </div>
        </div>
        <p class="showFonteSize">
          <strong>Escala:</strong>
          <%= @proposal.rotation.tipo_escala %>
        </p>


        <div class="panel panel-default">
          <div class="panel-heading imprecao-painel">
           <strong> Cargos e Salarios </strong>
         </div>
         <div class="panel-body">
          <div class="table-responsive ">
            <table class="table table-striped table-bordered table-hover ">
              <thead>
                <tr>
                  <th>Cargos</th>
                  <th>Salarios</th>
                  <th>Efetivo </th>
                  <th>Qtd. de postos</th>
                  <th>Funcionarios p/ escala</th>
                  <th>Salário efetivo</th>
                  <th>Salário efetivo total</th>

                </tr>
              </thead>
              <tbody>
                <% @proposal.proposal_roles.each do |proposal_role| %>
                <tr>
                  <!--td><%#= proposal_role.role.service.company.id %></td-->
                  <td class="showFonteSize">
                    <%= proposal_role.role.cargo %>
                  </td><!--Nome do cargo-->
                  <td class="showFonteSize" class="showFonteSize">
                    <%= number_to_currency(((@proposal.salario).round(2)*(((proposal_role.role.gratificacao.to_f).round(2)/100)+1)).round(2))%>
                  </td><!--Salario com a gratificação-->
                  <td class="showFonteSize">
                    <%= @proposal.efetivoIndivitual %><!--valor total do efetivo-->
                  </td>
                  <td class="showFonteSize">
                    <%=  proposal_role.qtd_postos %>
                  </td><!--Quandidade de funcionarios-->
                  <td class="showFonteSize">
                    <%=  @proposal.rotation.qtd_funcionarios*proposal_role.qtd_postos %><!--Quandidade de funcionarios-->
                  </td>
                  <td class="showFonteSize">
                    <%=  number_to_currency(((((@proposal.salario).round(2))*
                      (((proposal_role.role.gratificacao.to_f).round(2)/100)+1)).round(2)*
                      (@proposal.rotation.efetivo/@proposal.rotation.qtd_funcionarios.to_f)).round(2))%>
                    </td><!--Salario com  e efetivo-->
                    <td class="showFonteSize">
                      <%=  number_to_currency((((((proposal_role.role.salario.to_f)*(((proposal_role.role.gratificacao.to_f)/100)+1))*
                      (@proposal.rotation.efetivo/@proposal.rotation.qtd_funcionarios.to_f)))*(@proposal.rotation.qtd_funcionarios*proposal_role.qtd_postos)).round(2)) %>
                    </td><!--Quandidade de funcionarios-->
                  </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
            <!-- /.table-responsive -->
            <strong>Total de efetivo: </strong><%= (@proposal.efetivoTotal).round(2)%><br>
            <strong>Total dos salarios da escala: </strong><%= number_to_currency(@proposal.totalSalarios) %><br><!--Salarios totais-->
          </div>
          <!-- /.panel-body -->
        </div>

        <div class="panel panel-default">
          <div class="panel-heading imprecao-painel">
           <strong> Mão de obra direta </strong>
         </div>
         <div class="panel-body">
          <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover">
              <thead>
                <tr>
                  <th class="padraoEspaco1">Nome do campo de calculo:</th>
                  <th class="padraoEspaco2">Valor do campo p/ calculo:</th>
                  <th class="espaco-th">Total</th>
                </tr>
              </thead>

              <tbody>
                <tr>
                  <td class="showFonteSize"> Valor de Periculosidade:</td>  
                  <td class="showFonteSize">30% de <%=number_to_currency(@proposal.salario)%></td>
                  <td class="showFonteSize"><%=number_to_currency(0.3*@proposal.salario)%></td>
                </tr>
                <tr>
                  <td class="showFonteSize"> Valor de Insalubridade:</td>  
                  <td class="showFonteSize"><%=((@proposal.grau_de_insalubridade)*100).round(0)%>% de <%=number_to_currency(@select_all_calculation.salario_minimo)%></td> 
                  <td class="showFonteSize"><%=number_to_currency(@proposal.grau_de_insalubridade*@select_all_calculation.salario_minimo)%></td> 
                </tr>
                <tr>                 
                  <td class="showFonteSize"> Valor de calculo para proposta: <b>(<%= if @proposal.valorPeriOuIsalubri==0 
                    "Não"
                  else
                    "Sim"
                  end  %>)</b></td>  
                  <td class="showFonteSize"> Maior valor entre insalubridade e periculosidade </td>  
                  <td class="showFonteSize"> <%=number_to_currency(@proposal.valorPeriOuIsalubri)%>  </td>  
                </tr>
                <tr>                 
                  <td class="showFonteSize"> Hora extra intervalo e jornada: <b>(<%= if @proposal.totalHrExtras==0 
                      "Não"
                    else
                      "Sim"
                    end  %>)</b>
                  </td>  
                  <td class="showFonteSize"> <%= (@proposal.horas_extras).round(2)%> Hs</td>  
                  <td class="showFonteSize"> <%= number_to_currency(@proposal.totalHrExtras) %></td>  
                </tr>
                <tr>
                  <td class="showFonteSize"> Hora Feriado: <b>(<%= if @proposal.totalHrExtrasFeriado==0 
                    "Não"
                  else
                    "Sim"
                  end  %>)</b></td>  
                  <td class="showFonteSize"> <%= (((@proposal.vFeriadosDeCitiesMV*@proposal.doisTerco)+(@proposal.vFeriadosDeCitiesN*@proposal.umTerco)).round(2)).round(2) %> Hs</td>  
                  <td class="showFonteSize"> <%= number_to_currency(@proposal.totalHrExtrasFeriado) %></td>  
                </tr>
                <tr>
                  <td class="showFonteSize"> Horas Vespertino:</td>  
                  <td class="showFonteSize"> <%= (((@proposal.rotation.ad_vespertido_noturno*100)/@select_all_calculation.horasDeCalculoAdNoturno).round(4)*@proposal.rotation.fator_escala).round(4)%> Hs</td>  
                  <td class="showFonteSize"> <%= number_to_currency(@proposal.totalHrsAdNoturnoVespertino) %></td>  
                </tr>
                <tr>
                  <td class="showFonteSize"> Horas adicional noturno:</td>  
                  <td class="showFonteSize"> <%=(((@proposal.ajusteDivPorZero * 60)/@select_all_calculation.horasDeCalculoAdNoturno).round(6) * @proposal.rotation.fator_escala * @proposal.rotation.ad_noturno).round(4) %> Hs</td>  
                  <td class="showFonteSize"> <%= number_to_currency(@proposal.totalHrsAdNoturno) %></td>  
                </tr>
                <tr>
                  <td class="showFonteSize">Reflexos sobre a folha - DSR:</td>  
                  <td class="showFonteSize"> <%= %> </td>  
                  <td class="showFonteSize"> <%= number_to_currency(@proposal.refexoDSR) %></td>  
                </tr>
                <tr>
                  <td class="showFonteSize"> Salário médio base final:</td>  
                  <td class="showFonteSize"> <%= %> </td>  
                  <td class="showFonteSize"> <%= number_to_currency(@proposal.salarioMedioFinal) %></td>  
                </tr>
              </tbody>
            </table>
            <p class="showFonteSize">
              <strong>Total massa salarial: </strong><%=number_to_currency((@proposal.massaSalarial).round(2))%>
            </p>
          </div>
        </div>
      </div>

      <div class="panel panel-default">
        <div class="panel-heading imprecao-painel">
          <strong> Obrigações sociais sobre massa salarial:</strong>
          </div>
          <div class="panel-body">
            <div class="table-responsive">
              <table class="table table-striped table-bordered table-hover">
                <thead>
                  <tr >
                    <th class="padraoEspaco1">Tributo: </th>
                    <th class="padraoEspaco2">Valor em porcentagem: </th>
                    <th class="espaco-th">Valor total sobre massa salarila:</th>
                  </tr>
                </thead>
                <tbody> 
                  <tr>
                    <td class="showFonteSize">FGTS</td>
                    <td class="showFonteSize"><%=@select_all_calculation.fgts%><span>%</span></td>
                    <td class="showFonteSize"><%=number_to_currency(@proposal.obgFGTS)%></td>
                  </tr>
                  <tr>
                    <td class="showFonteSize">INSS</td>
                    <td class="showFonteSize"><%=@select_all_calculation.inss%><span>%</span></td>
                    <td class="showFonteSize"><%=number_to_currency(@proposal.obgINSS)%></td>
                  </tr>
                  <tr>
                    <td class="showFonteSize">Seguro acidente de trabalho</td>
                    <td class="showFonteSize"><%=@proposal.seguro_aci_trabalho%><span>%</span></td>
                    <td class="showFonteSize"><%=number_to_currency(@proposal.obgseguro_aci_trabalho)%></td>
                  </tr>
                  <tr>
                    <td class="showFonteSize">Sebrae</td>
                    <td class="showFonteSize"><%=@select_all_calculation.sebrae%><span>%</span></td>
                    <td class="showFonteSize"><%=number_to_currency(@proposal.obgsebrae)%></td>
                  </tr>
                  <tr>
                    <td class="showFonteSize">Incra</td>
                    <td class="showFonteSize"><%=@select_all_calculation.incra%><span>%</span></td>
                    <td class="showFonteSize"><%=number_to_currency(@proposal.obgincra)%></td>
                  </tr>
                  <tr>
                    <td class="showFonteSize">Salario educação</td>
                    <td class="showFonteSize"><%=@select_all_calculation.salario_educacao%><span>%</span></td>
                    <td class="showFonteSize"><%=number_to_currency(@proposal.obgsalario_educacao)%></td>
                  </tr>
                  <tr>
                    <td class="showFonteSize">Sesc</td>
                    <td class="showFonteSize"><%=@select_all_calculation.sesc%><span>%</span></td>
                    <td class="showFonteSize"><%=number_to_currency(@proposal.obgsesc)%></td>
                  </tr>
                  <tr>
                    <td class="showFonteSize">Senac</td>
                    <td class="showFonteSize"><%=@select_all_calculation.senac%><span>%</span></td>
                    <td class="showFonteSize"><%=number_to_currency(@proposal.obgsenac)%></td>
                  </tr>
                </tbody>
              </table>
              <p class="showFonteSize">
                <strong>Total de encargos diretos: </strong><%=number_to_currency(@proposal.totalIncargosDiretos)%>
              </p>
            </div>
          </div>
        </div>

        <div class="page-break">
        </div>

        <div class="panel panel-default">
          <div class="panel-heading imprecao-painel">
            <strong>Provisões sobre massa salarial:</strong>
          </div>
          <div class="panel-body">
            <div class="table-responsive">
              <table class="table table-striped table-bordered table-hover">
                <thead>
                  <tr >
                    <th class="padraoEspaco1">Provisões </th>
                    <th class="padraoEspaco2">Valor em porcentagem: </th>
                    <th class="espaco-th">Valor total sobre provisões salarila:</th>
                  </tr>
                </thead>
                  <tbody> 
                    <tr>
                      <td class="showFonteSize">1/12 de Ferias</td>
                      <td class="showFonteSize"><%=@select_all_calculation.ferias%><span>%.</span></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totalferiasUmDozeAvos)%></td>
                    </tr>
                    <tr>
                      <td class="showFonteSize">1/3 constitucional (1/12 avos)</td>
                      <td class="showFonteSize"><%=@select_all_calculation.um_terco_constitucional%><span>%.</span></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totalumTercoConstiUmDozeAvos)%></td>
                    </tr>
                    <tr>
                      <td class="showFonteSize"> 13º salario (1/12 avos)</td>
                      <td class="showFonteSize"><%=@select_all_calculation.decimo_terceito%><span>%.</span></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totaldecimoTerceiroUmDozeAvos)%></td>
                    </tr>
                    <tr>
                      <td class="showFonteSize">Inss Sob Ferias, 1/3 constitucional (1/12 avos), 13º salario (1/12 avos) </td>
                      <td class="showFonteSize"><%=@select_all_calculation.pct_inss_sob_soma_decimo_um_terco_ferias%><span>%.</span></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totalInssSobFeriasUnTercoDecimo)%></td>
                    </tr>
                    <tr>
                      <td class="showFonteSize"> FGTS 13º, salario 1/3 constitucional, (1/12 avos), 13º salario (1/12 avos)</td>
                      <td class="showFonteSize"><%=@select_all_calculation.pct_fgts_sob_soma_decimo_um_terco_ferias%><span>%.</span></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totalFgtsSobFeriasUnTercoDecimo)%></td>
                    </tr>
                    <tr>
                      <td class="showFonteSize">Aviso previo</td>
                      <td class="showFonteSize"><%=@select_all_calculation.aviso_previo%><span>%.</span></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totalAvisoPrevio)%></td>
                    </tr>
                    <tr>
                      <td class="showFonteSize"> FGTS indenização sobre FGTS total</td>
                      <td class="showFonteSize"><%=@select_all_calculation.indenizacao_fgts%><span>%.</span></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totalIndenizacaoSobreFGTS)%></td>
                    </tr>
                    <tr>
                      <td class="showFonteSize">Reserva tecnica (campo a acertar)</td>
                      <td class="showFonteSize"><%=@proposal.company.pct_reserva_tecnica%><span>%.</span></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.reservaTecnica)%></td>
                    </tr>
                  </tbody>
                </table>
                <strong>Total de provisões sobre massa salarial: </strong><%=number_to_currency(@proposal.totalProvisaoMassaSalarial+@proposal.reservaTecnica)%>
              </div>
            </div>
          </div>

          <div class="panel panel-default">
            <div class="panel-heading imprecao-painel">
              <strong>Benefícios dos Funcionário:</strong>
            </div>  
            <div class="panel-body">
              <div class="table-responsive">
                <table class="table table-striped table-bordered table-hover">
                  <thead>
                    <tr>
                      <th class="padraoEspaco1">Beneficio </th>
                      <th >Quantidade </th>
                      <th class="padraoEspaco2">valor unitario: </th>
                      <th class="espaco-th-Ececao">Valor total:</th>
                    </tr>
                  </thead>

                  <tbody> 
                    <tr>
                      <td class="showFonteSize">Cesta</td>
                      <td class="showFonteSize"><%=@proposal.efetivoTotal%></td>
                      <td class="showFonteSize"><%=@proposal.v_cesta%></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totCesta)%></td>
                    </tr>
                    <tr>
                      <td class="showFonteSize">Vale refeição.</td>
                      <td class="showFonteSize"><%=@proposal.multiplicador %></td>
                      <td class="showFonteSize"><%=@proposal.vUniVR%></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totVR)%></td>
                    </tr>
                    <tr>
                      <td class="showFonteSize">Assitencia medica</td>
                      <!--<%#if %>-->
                      <td class="showFonteSize"><%=@proposal.efetivoTotal%></td>
                      <td class="showFonteSize"><%=@proposal.vUniAssistMedica%></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totAssiteciaMedica)%></td>
                    </tr>
                    <tr>
                      <td class="showFonteSize">Seguro de vida</td>
                      <td class="showFonteSize"><%=@proposal.efetivoTotal%></td>
                      <td class="showFonteSize"><%=@proposal.vUniSegVida%></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totSeguroVida)%></td>
                    </tr>
                    <tr>
                      <td class="showFonteSize">Vale Trasporte.</td>
                      <td class="showFonteSize"><%=(2*@proposal.qtd_vt_pagas.to_f).round(1)%></td>
                      <td class="showFonteSize"><%=@proposal.city.valeTransporte%></td>
                      <td class="showFonteSize"><%= if @proposal.totVT == 0.00
                                                      "Não será cobrado"
                                                    else
                                                      number_to_currency(@proposal.totVT)
                                                    end %></td>
                    </tr>
                    <tr>
                      <td class="showFonteSize">Reciclagem</td>
                      <td class="showFonteSize"><%=@proposal.efetivoTotal%></td>
                      <td class="showFonteSize"><%=@proposal.rotation.v_reciclagem%></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totReciclagem)%></td>
                    </tr>
                    <% if @proposal.company.id == 2 || @proposal.company.id == 3 %>  
                    <tr>
                      <td class="showFonteSize">PPR</td>
                      <td class="showFonteSize"><%=@proposal.efetivoTotal%></td>
                      <td class="showFonteSize"><%=@proposal.vUniPpr%></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totPpr)%></td>
                    </tr>
                    <%end%>
                    <tr>
                      <td class="showFonteSize">Uniformes</td>
                      <td class="showFonteSize"><%=@proposal.efetivoTotal%></td>
                      <td class="showFonteSize"><%=@proposal.vUniUniforme%></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totUniforme)%></td>
                    </tr>
                    <% if @proposal.company.id == 3%> 
                    <tr>
                      <td class="showFonteSize"> Assistência Social Familiar</td>
                      <td class="showFonteSize"><%=@proposal.efetivoTotal%></td>
                      <td class="showFonteSize"><%=@proposal.v_Soc_Familiar%></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totSocialFamiliar)%></td>
                    </tr>
                    <tr>
                      <td class="showFonteSize"> Benefício Natalidade</td>
                      <td class="showFonteSize"><%=@proposal.efetivoTotal%></td>
                      <td class="showFonteSize"><%=@proposal.v_ben_Natalidade%></td>
                      <td class="showFonteSize"><%=number_to_currency(@proposal.totBenNatalidade)%></td>
                    </tr>
                    <%end%>
                  </tbody>
                </table>
                <strong>Total dos beneficios: </strong><%=number_to_currency(@proposal.totBeneficios)%>
              </div>
            </div>
          </div>

          <h4><strong>Total mão de obra:</strong><%= number_to_currency((@proposal.totMaoDeObra).round(2))%></h4>
          <hr/>

          <h3 class="page-header topHeader">Taxas, Serviços e Impostos sobre Vendas</h3>

          <strong>Índice operacional </strong><%=number_to_currency((@proposal.valorIndiceOperacional).round(2))%><br>
          <strong>Índice administrativo </strong><%=number_to_currency((@proposal.valorIndiceAdministrativo).round(2))%><br>

          <% if @proposal.proposal_equipaments.blank?%>
          <div class="page-break">
          </div>
          <% end %>

          <% if @proposal.proposal_equipaments.blank?%>
          <div class="panel panel-default" style="display:none">
          </div>
          <% else%>
          <div class="panel panel-default">
            <div class="panel-heading imprecao-painel">
              <strong>Equipamentos e acessorios:</strong>
            </div>  
            <div class="panel-body">
              <div class="table-responsive">
                <table class="table table-striped table-bordered table-hover">

                  <thead>
                    <tr>
                      <th class="padraoEspaco1">Nome do equipamento</th>
                      <th class="padraoEspaco2">Quantidade de equipamento</th>
                      <th class="espaco-th">Preço total</th>
                    </tr>
                  </thead>

                  <tbody>
                    <% @proposal.proposal_equipaments.each do |proposal_equipament| %>
                    <tr> 
                      <% unless proposal_equipament.equipament.name_equipament.blank? %>
                      <td class="showFonteSize"><%= proposal_equipament.equipament.name_equipament %></td>
                      <td class="showFonteSize"><%= proposal_equipament.quantidade %></td>
                      <td class="showFonteSize"><%= number_to_currency((@proposal.totalEquipamento))%></td>
                      <% else %>
                      <td class="showFonteSize"></td>
                      <td class="showFonteSize"></td>
                      <td class="showFonteSize"></td>
                      <% end %>  
                    </tr>
                    <% end %>
                  </tbody>
                </table>
                <strong>Valor Total dos equipamentos:</strong><%= number_to_currency(@proposal.totalEquipamento) %>
              </div>
            </div>
          </div>
          <% end %>
          <% unless @proposal.proposal_equipaments.blank?%>
          <div class="page-break">
          </div>
          <% end %>
          <div class="panel panel-default">
            <div class="panel-heading imprecao-painel">
              <strong>Impostos </strong>
            </div>
              <div class="panel-body">
                <div class="table-responsive">
                  <table class="table table-striped table-bordered table-hover">
                    <thead>
                      <tr>
                        <th class="padraoEspaco1">Nome do campo de calculo:</th>
                        <th class="padraoEspaco2">Valor do campo p/ calculo:</th>
                      <th class="espaco-th">Total</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <td class="showFonteSize">Valor Issqn:</td>                
                      <td class="showFonteSize"><%= @proposal.city.issqn %><span>%</span></td>  
                      <td class="showFonteSize"><%= number_to_currency((@proposal.valorIssqn).round(2)) %></td>
                    </tr>
                    <tr>                 
                      <td class="showFonteSize">Valor Irrf:</td>  
                      <td class="showFonteSize"><%= @proposal.company.irrf %><span>%</span></td>
                      <td class="showFonteSize"><%= number_to_currency((@proposal.valorIrrf).round(2)) %></td>  
                    </tr>
                    <tr>                 
                      <td class="showFonteSize">Valor Csll:</td>  
                      <td class="showFonteSize"><%= @proposal.company.csll %><span>%</span></td>  
                      <td class="showFonteSize"><%= number_to_currency((@proposal.valorCsll).round(2)) %></td>  
                    </tr>
                    <tr>
                      <td class="showFonteSize">Valor Pis:</td>  
                      <td class="showFonteSize"><%= @proposal.company.pis %><span>%</span></td>
                      <td class="showFonteSize"><%= number_to_currency((@proposal.valorPis).round(2)) %></td>
